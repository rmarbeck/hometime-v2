@(supported_brands: Optional[List[Brand]])(implicit request: Http.Request, messages: play.i18n.Messages)<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    <url>
      <loc>https://@{request.host()}/</loc>
      <changefreq>daily</changefreq>
      <priority>1.0</priority>
    </url>
    <url>
      <loc>https://@{request.host()}@routes.HomeController.indexAlternate()</loc>
      <changefreq>daily</changefreq>
      <priority>1.0</priority>
    </url>
    <url>
      <loc>https://@{request.host()}@routes.HomeController.offers()</loc>
      <changefreq>daily</changefreq>
      <priority>1.0</priority>
    </url>
    <url>
      <loc>https://@{request.host()}@routes.HomeController.quotationOptions()</loc>
      <changefreq>daily</changefreq>
      <priority>1.0</priority>
    </url>
    <url>
      <loc>https://@{request.host()}@routes.HomeController.content("content.watchmakers")</loc>
      <changefreq>monthly</changefreq>
      <priority>0.8</priority>
    </url>
    <url>
      <loc>https://@{request.host()}@routes.HomeController.content("content.rolex.repairing")</loc>
      <changefreq>daily</changefreq>
      <priority>0.9</priority>
    </url>
    <url>
      <loc>https://@{request.host()}@routes.HomeController.content("content.omega.repairing")</loc>
      <changefreq>daily</changefreq>
      <priority>0.9</priority>
    </url>
    <url>
      <loc>https://@{request.host()}@routes.HomeController.content("content.authenticating")</loc>
      <changefreq>daily</changefreq>
      <priority>0.9</priority>
    </url>
    <url>
      <loc>https://@{request.host()}@routes.HomeController.prices()</loc>
      <changefreq>monthly</changefreq>
      <priority>0.8</priority>
    </url>
    <url>
      <loc>https://@{request.host()}@routes.HomeController.content("content.service.repairing")</loc>
      <changefreq>daily</changefreq>
      <priority>0.9</priority>
    </url>
    <url>
      <loc>https://@{request.host()}@routes.HomeController.content("content.water.repairing")</loc>
      <changefreq>daily</changefreq>
      <priority>0.9</priority>
    </url>
    <url>
      <loc>https://@{request.host()}@routes.FormProcessingController.prepareServiceTestRequest()</loc>
      <changefreq>monthly</changefreq>
      <priority>0.7</priority>
    </url>
    <url>
      <loc>https://@{request.host()}@routes.FormProcessingController.prepareBuyRequest()</loc>
      <changefreq>monthly</changefreq>
      <priority>1.0</priority>
    </url>
    <url>
      <loc>https://@{request.host()}@routes.FormProcessingController.prepareQuotationRequest()</loc>
      <changefreq>monthly</changefreq>
      <priority>1.0</priority>
    </url>
    <url>
      <loc>https://@{request.host()}@routes.FormProcessingController.prepareAutoQuotation()</loc>
      <changefreq>monthly</changefreq>
      <priority>1.0</priority>
    </url>
    @for(brand <- supported_brands.get) {
    <url>
      <loc>https://@{request.host()}@routes.FormProcessingController.prepareQuotationRequestWithBrand(brand.seo_name)</loc>
      <changefreq>monthly</changefreq>
      <priority>1.0</priority>
    </url>
    <url>
      <loc>https://@{request.host()}@routes.FormProcessingController.prepareAutoQuotationWithBrand(brand.seo_name)</loc>
      <changefreq>monthly</changefreq>
      <priority>1.0</priority>
    </url>
    }
    <url>
      <loc>https://@{request.host()}@routes.FormProcessingController.prepareQuartzPrice()</loc>
      <changefreq>monthly</changefreq>
      <priority>1.0</priority>
    </url>
    @for(brand <- supported_brands.get) {@if(brand.getQuartzCategory!=0) {
    <url>
      <loc>https://@{request.host()}@routes.FormProcessingController.prepareQuartzPriceWithBrand(brand.seo_name)</loc>
      <changefreq>monthly</changefreq>
      <priority>1.0</priority>
    </url>
    }}
    <url>
      <loc>https://@{request.host()}@routes.HomeController.feedbacks()</loc>
      <changefreq>daily</changefreq>
      <priority>0.5</priority>
    </url>
    <url>
      <loc>https://@{request.host()}@routes.HomeController.content("content.who.are.we")</loc>
      <changefreq>daily</changefreq>
      <priority>0.5</priority>
    </url>
    <url>
      <loc>https://@{request.host()}@routes.HomeController.visit()</loc>
      <changefreq>daily</changefreq>
      <priority>0.5</priority>
    </url>
    <url>
      <loc>https://@{request.host()}@routes.HomeController.content("content.legal")</loc>
      <changefreq>daily</changefreq>
      <priority>0.5</priority>
    </url>
    <url>
      <loc>https://@{request.host()}@routes.HomeController.faq()</loc>
      <changefreq>daily</changefreq>
      <priority>1</priority>
    </url>
    <url>
      <loc>https://@{request.host()}@routes.HomeController.content("content.privacy")</loc>
      <changefreq>daily</changefreq>
      <priority>0.4</priority>
    </url>
    <url>
      <loc>https://@{request.host()}@routes.FormProcessingController.prepareContactRequest()</loc>
      <changefreq>daily</changefreq>
      <priority>0.3</priority>
    </url>
    <url>
      <loc>https://@{request.host()}@routes.FormProcessingController.prepareCallBackRequest()</loc>
      <changefreq>daily</changefreq>
      <priority>0.3</priority>
    </url>
</urlset>